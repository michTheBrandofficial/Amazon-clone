{
  "compilerOptions": {
    "paths": {},
    "resolveJsonModule": true,
    "baseUrl": ".",
    "jsx": "react-jsxdev",
    "jsxImportSource": "nixix/types",
    "types": [
      "nixix/dom",
      "nixix/hoc",
      "nixix/primitives",
      "nixix/router"
    ],
    "module": "es2022",
    "target": "es2022",
    "checkJs": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node"
  },
  "include": ["./**/*"]
}